{
  "version": "1.0.0-alpha10",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "define": [
      "CORE_CLR",
      "DOTNETCORE",
      "EXPLICIT_DEPENDENCIES"
    ],
    "emitEntryPoint": false,
    "xmlDoc": true,
    "delaySign": true,
    "additionalArguments": [
      "--warn:3",
      "--fullpaths",
      "--flaterrors",
      "--warnon:1182"
    ],
    "compile": {
      "includeFiles": [
        "../../../paket-files/matthid/Yaaf.FSharp.Scripting/src/source/Yaaf.FSharp.Scripting/YaafFSharpScripting.fs",
        "AssemblyInfo.fs",
        "LegacyApiHelper.fs",
        "Environment.fs",
        "Path.fs",
        "String.fs",
        "Trace.fs",
        "ScriptRunner.fs",
        "HashGeneration.fs",
        "CoreCache.fs",
        "FakeRuntime.fs"
      ]
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "dependencies": {
    "Fake.Core.Context": "1.0.0-alpha10",
    "Paket.Core.netcore": "3.10.0-alpha001",
    "FSharp.Core": "4.0.1.7-alpha",
    "FSharp.Compiler.Service": "6.0.3-alpha1",
    "Mono.Cecil": "0.9.6.0"
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_5"
        ]
      },
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ],
      "dependencies": {
        "System.Diagnostics.TraceSource": "4.0.0",
        "System.Reflection": "4.1.0",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Reflection.Primitives": "4.0.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.Loader": "4.0.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Security.Cryptography.Primitives": "4.0.0",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}